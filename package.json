{
  "name": "@sitearcade/next-api-router",
  "version": "0.1.2",
  "description": "NextJS API router, so you can easily split up funcs properly.",
  "license": "MIT",
  "homepage": "https://www.sitearcade.com",
  "repository": "github:sitearcade/next-api-router",
  "bugs": "https://github.com/sitearcade/next-api-router/issues",
  "author": {
    "name": "Mike Stop Continues",
    "email": "mike@sitearcade.com",
    "url": "https://www.sitearcade.com"
  },
  "sideEffects": false,
  "source": "src/index.js",
  "main": "./dist/index.js",
  "jsnext:main": "./dist/index.modern.js",
  "module": "./dist/index.module.js",
  "syntax": {
    "esmodules": "./dist/index.modern.js"
  },
  "exports": {
    ".": {
      "browser": "./dist/index.module.js",
      "import": "./dist/index.module.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "microbundle watch --no-compress -f cjs,esm,modern --jsx React.createElement --jsxImportSource react",
    "build": "microbundle --no-compress -f cjs,esm,modern --jsx React.createElement --jsxImportSource react",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=testing jest --passWithNoTests --coverage",
    "test:ci": "NODE_ENV=testing jest --passWithNoTests --ci",
    "test:watch": "NODE_ENV=testing jest --passWithNoTests --coverage --watch",
    "preversion": "npm run build && npm test",
    "postversion": "npm publish --access public",
    "prepublishOnly": "npm run clean && npm run build",
    "postpublish": "git push origin --all; git push origin --tags"
  },
  "dependencies": {
    "@sitearcade/is-env": "^0.1.0",
    "@sitearcade/request": "^0.4.0",
    "cors": "^2.8.5",
    "github-webhook-middleware": "^0.0.2",
    "ramda": "^0.27.1",
    "url-pattern": "^1.0.3"
  },
  "peerDependencies": {
    "next": "^10.0.7"
  },
  "devDependencies": {
    "@sitearcade/babel-preset": "0.5.0",
    "@sitearcade/browserslist-config": "0.3.0",
    "@sitearcade/eslint-config": "1.1.7",
    "@sitearcade/jest-preset": "2.1.1",
    "jest": "26.6.3",
    "microbundle": "0.13.0",
    "next": "10.0.7",
    "prettier": "2.2.1"
  },
  "engines": {
    "node": ">=10"
  },
  "babel": {
    "presets": [
      "module:@sitearcade/babel-preset"
    ]
  },
  "browserslist": [
    "extends @sitearcade/browserslist-config"
  ],
  "eslintConfig": {
    "extends": [
      "@sitearcade/eslint-config"
    ]
  },
  "jest": {
    "preset": "@sitearcade/jest-preset"
  }
}
